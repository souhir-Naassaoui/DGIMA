<h2><center>Liste des enseignants</center></h2>
  <div class="container">
    <div class="row margin-bottom-3">

      <div class="col-sm-90">
<table class="table table-bordered">
    <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Edit</th>
          <th scope="col">Delete</th>
        </tr>
    </thead>
    <tbody>
    <% for (var i = 0; i < users.length; i++) { %>
        <% if(users[i].enseignant == 1){ %>
      <tr>
        <td><%= users[i].name %></td>
        <td><%= users[i].email %></td>
        
        <td><a href="useredit/<%= users[i].email %> " class="btn btn-warning glyphicon glyphicon-pencil "> Edit</a></td>
        <td><a href="userdelete/<%= users[i]._id %> " class="btn btn-danger  fa fa-trash "> Delete</a></td>
      </tr>
      <% }} %> 

    </tbody>
  </table>
  </div>
  </div>
  </div>
  </body>