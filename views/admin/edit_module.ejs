<center><h1 class="page-title">Edit A Module</h1></center>
<br><br>

<form method="post" action="/admin/modules/edit-module/<%= title %>">
    
    <div class="form-group">
        <label for="">Title</label>
        <input type="text" class="form-control" name="title" value="<%= title %>" placeholder="Title">
    </div>
    <div class="form-group">
        <label for="">Section</label>
        <input type="text" class="form-control" name="section" value="<%= section %>" placeholder="Section">
    </div>
    <div class="form-group">
        <label for="">Semesttre</label>
        <input type="text" class="form-control" name="semestre" value="<%= semestre %>" placeholder="Semestre">
    </div>
    

    <input type="hidden" name="id" value="<%= id %>">

    <button class="btn btn-primary">Submit</button>
</form>


