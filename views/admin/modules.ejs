<center><h1 class="page-title">Modules</h1></center>
<br><br>

<table class="table table-striped ">

    <thead>
        <tr class="home">
            <th>Module</th>
            <th>Section</th>
            <th>Semestre</th>
            <th>Matieres</th>
            <th>Edit</th>
            <th>Delete</th>

        </tr>
    </thead>
    <tbody>
        <% modules.forEach(function(module){ %>
        <tr>
            <td><%= module.title %></td>
            <td><%= module.section %></td>
            <td><%= module.semestre %></td>
            <td><a href="/c/<%= module.slug %>">Voir matieres</a></td>
            <td><a href="/admin/modules/edit-module/<%= module.title %>">Edit</a></td>

            <td><a class="confirmDeletion btn btn-danger  fa fa-trash " href="/admin/modules/delete-module/<%= module._id %>"> Delete</a></td>
    
        </tr>
        <% }); %>
    </tbody>
</table>



