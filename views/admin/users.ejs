<h2>Liste des etudiants</h2>
<table class="table table-bordered">
    <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Edit</th>
          <th scope="col">Delte</th>
        </tr>
    </thead>
    <tbody>
    <% for (var i = 0; i < users.length; i++) { %>
      <% if(users[i].enseignant == null && users[i].admin == null ){ %>

      <tr>
        <td><%= users[i].name %></td>
        <td><%= users[i].email %></td>
        <td><a href="useredit/<%= users[i].email %> " class="btn btn-warning glyphicon glyphicon-pencil  "> Edit</a></td>
        <td><a href="userdelete/<%= users[i]._id %> " class="btn btn-danger fa fa-trash "> Delete</a></td>
      </tr>
      <% } } %> 

    </tbody>
  </table>
