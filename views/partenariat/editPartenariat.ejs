<div class="container">
   

    <form action="/partners" method="post" autocomplete="off">
        <input type="hidden" name="_id" value="<%=partenaire._id%>">

        <div>
            <label for="inputEmail4">Nom du partenaire économique / Institut de recherche</label>
            <br>
            <input type="text" name="nom" class="form-control"  placeholder="xxxxx" value="<%=partenaire.nomPartenaire%>">
            <br>
            <div class="text-danger">
                <%=partenaire.nomPartenaireError%>
            </div>
        </div>

        <div>
            <label for="inputEmail4">L'objet de l'accord</label>
            <br>
            <input type="text" name="objet" class="form-control" value="<%=partenaire.objetAccord%>" placeholder="xxxxx">
            <br>
            <div class="text-danger">
                <%=partenaire.objetAccordError%>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="inputPassword4">Type d'accord</label>
                <br>
                <input type="text" class="form-control" name="type"  placeholder="xxxxx" value="<%=partenaire.typeAaccord%>">
                <br>
                <div class="text-danger">
                    <%=partenaire.typeAaccordError%>
                </div>
            </div>
            <div class="form-group col-md-4">
                <label>Date de signature de l'accord</label>
                <br>
                <input type="Date" class="form-control"  name="date" value="<%=partenaire.dateSignature%>">
                <br>
                <div class="text-danger">
                    <%=partenaire.dateSignatureError%>
                </div>
            </div>
            <div class="form-group col-md-2">
                <label for="inputPassword4">La durée de l'accord</label>
                <br>
                <input type="number" class="form-control" name="duree" value="<%=partenaire.dureAccord%>"  placeholder="xxxxx">
                <br>
                <div class="text-danger">
                    <%=partenaire.dureeAccordError%>
                </div>
            </div>
        </div>


        <button type="submit" class="btn btn-primary">Editer</button>
        <a href="/partners/partenaires" class="btn btn-secondary"><i class="fa fa-list-alt"></i> Voir tout</a>



    </form>
</div>



