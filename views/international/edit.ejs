  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

<div class="container">

    <form action="/infos" method="post" autocomplete="off">
        <input type="hidden" name="_id" value="<%=ecole._id%>">

        <div class="form-row">
            <div class="form-group col-md-6">
                <label>PAYS DONT RELEVE L'INSTITUTION PARTENAIRE</label>
                <input type="text" class="form-control" value="<%=ecole.pays%>" name="pays" >
                <div class="text-danger">
                    <%=ecole.paysError%>
                </div>
            </div>
            <div class="form-group col-md-6">
                <label for="inputPassword4">INSTITUTION PARTENAIRE</label>
                <input type="text" class="form-control" name="institution" value="<%=ecole.institution%>" placeholder="xxxxx">
                <div class="text-danger">
                    <%=ecole.institutionError%>
                </div>
            </div>
        </div>

        <div class="form-row">
            <div class="form-group col-md-6">
                <label>Date de signature de l'accord</label>
                <input type="Date" class="form-control" value="<%=ecole.dateSignature%>" name="dateSignature" >
                <div class="text-danger">
                    <%=ecole.dateSignatureError%>
                </div>
            </div>
            <div class="form-group col-md-6">
                <label for="inputPassword4">La dur√©e de l'accord</label>
                <input type="text" class="form-control" name="dureeValidite" value="<%=ecole.dureeValidite%>" placeholder="xxxxx">
                <div class="text-danger">
                    <%=ecole.dureeValiditeError%>
                </div>
            </div>
        </div>

        <div>

        </div>


        <div>
            <button type="submit" class="btn btn-primary">Ajouter</button>
            <a href="/infos//mobilite" class="btn btn-secondary"><i class="fa fa-list-alt"></i> View All</a>
        </div>

    </form>
</div>

